[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/pythonprojeckdjango
ExecStart=/home/ubuntu/pythonprojeckdjango/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.socket \
          aip_web_project.wsgi:application
[Install]
WantedBy=multi-user.target